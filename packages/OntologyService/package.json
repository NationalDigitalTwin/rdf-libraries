    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest-fetch-mock": "^3.0.3",
    "remove": "^0.1.5",
    "testcontainers": "^10.10.4",
    "ts-jest": "^29.1.1",
    "tsc": "^2.0.4",
    "typedoc": "^0.26.7",
    "typescript": "^5.2.2",
