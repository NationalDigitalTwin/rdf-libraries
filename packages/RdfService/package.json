{
  "name": "@telicent-oss/rdfservice",
  "version": "0.31.1-rc1",
  "private": false,
  "description": "RdfService is a helper library that abstracts away the complexity of interacting with RDF triplestores, providing basic CRUD abilities.",
  "main": "./dist/rdfservice.mjs",
  "files": [
    "dist"
  ],
  "module": "./dist/rdfservice.mjs",
  "exports": {
    ".": {
      "import": "./dist/rdfservice.mjs",
      "require": "./dist/rdfservice.umd.js",
      "types": "./dist/types.d.ts"
    }
  },
  "types": "./dist/types.d.ts",
  "scripts": {
    "generate-docs": "./node_modules/.bin/typedoc --out docs .",
    "open-docs": "open ./docs/index.html",
    "test": "jest test",
    "build": "rm -rf ./dist && vite build",
    "publish": "npm publish --access public --provenance",
    "publish:dry-run": "npm publish --access public --provenance --dry-run",
    "lint": "eslint **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telicent-oss/rdf-libraries.git"
  },
  "publishConfig": {
    "registry": "https://www.npmjs.com/"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest-fetch-mock": "^3.0.3",
    "remove": "^0.1.5",
    "testcontainers": "^10.10.4",
    "ts-jest": "^29.1.1",
    "tsc": "^2.0.4",
    "typedoc": "^0.26.7",
    "typescript": "^5.2.2",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^4.2.3",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "global": "^4.4.0",
    "zod": "^3.22.4"
  }
}
